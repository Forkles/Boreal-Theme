{
    "name": "Boreal",
    "type": "dark",
    "colors": {
        "editor.background": "#050D16",
        "editor.foreground": "#E1E8EE",

        "window.activeBorder": "#1F2933",
        "focusBorder": "#78A5C5",

        "activityBar.background": "#050D16",
        "activityBar.foreground": "#78A5C5",
        "activityBar.border": "#050D16",
        "activityBar.activeBorder": "#78A5C5",
        "activityBar.inactiveForeground": "#4F5B66",
        "activityBarBadge.background": "#78A5C5",
        "activityBarBadge.foreground": "#050D16",

        "sideBar.background": "#050D16",
        "sideBar.foreground": "#E1E8EE",
